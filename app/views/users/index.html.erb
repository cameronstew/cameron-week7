<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td>
          <%= link_to user.user_name %>
        </td>
        <td>
          <%= link_to 'Edit', edit_user_path(user) %>
        </td>
        <td>
          <%= link_to "Delete", user_path(user), method: 'delete' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
